    "start": "node server.js --env=dev",
    "stats": "webpack --profile --json > stats.json",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "dist": "npm run clean & npm run copy  & node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --env=dist",
    "distpub": "npm run cleanpublic",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico  ./dist",
    "copydll": "copyfiles -f ./dll/dll.vendor.js   ./dist/dll",
    "clean": "rimraf ./dist/*",
    "cleanpublic": "rimraf ../src/main/resources/public/* & git commit file:../src/main/resources/public -m \"commit delete operation for public\" ",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags"
